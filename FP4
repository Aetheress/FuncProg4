(def students {"Inese" 10 "Vasja" 8 "Petja" 4 "Natasha" 7
"Anja" 10 "Lauris" 6 "Sandra" 8 "KriÅ¡janis" 9})
;;get ONLY pairs where the first element ends with a
(use 'clojure.string)
(ends-with? "Inese" "a")
(filter (fn [[k v]]
  (ends-with? k "a"))
    students);
;;example
(filter (fn [[k v]] (even? k))
    {1 "a", 2 "b", 3 "c", 4 "d"})
